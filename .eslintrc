{
  "extends": ["airbnb-base"],
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "import/no-unresolved": [0],
    "import/no-extraneous-dependencies": [0],
    "import/extensions": [0],
    "no-use-before-define": [1, "nofunc"],
    "semi": [2, "never"],
    "comma-dangle": [2, "never"],
    "space-before-function-paren": [2, "always"],
    "no-undef": [1],
    "no-unused-vars": [1]
  }
}
